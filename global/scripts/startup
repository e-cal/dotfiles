#!/usr/bin/env bash

launch-bar &
dropbox &
redshift &
eyes &
yay -Syyy &
config-wacom m &

sleep 30
cloud backup &
cloud download all >~/.local/share/rclone/startup-sync.log &
#cloud autosync &

for i in {0..10}; do
	sleep 5
	polybar-msg -p "$(pgrep -f "polybar $(get-bar)")" hook volume 1 &>/dev/null
done
