#!/usr/bin/env bash

teams=""
slack=""
discord="ﭮ"
zoom=""

options="$slack\n$teams\n$discord\n$zoom"


sel=`echo -e "$options" | rofi -no-fixed-num-lines -theme $HOME/.config/rofi/commmenu.rasi -dmenu`


case $sel in
    $teams)
	    teams
        ;;
    $slack)
	    slack
        ;;
    $discord)
        discord
        ;;
    $zoom)
        zoom
        ;;
esac
